{
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/",
            "dest": "api/index.js"
        },
        {
            "src": "/(.+)",
            "dest": "api/index.js"
        }
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "https://github.com/Jurredr/github-widgetbox"
        }
    ],
    "cleanUrls": true
}
